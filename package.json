{
  "name": "medplum-health-card-demo",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "build:bots": "rimraf data/core && npm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && npx ts-node --project tsconfig-deploy.json src/scripts/build-bots.ts",
    "deploy:bots": "npx ts-node --project tsconfig-deploy.json src/scripts/deploy-bots.ts",
    "deploy:bots-and-operation": "npx ts-node --project tsconfig-deploy.json src/scripts/deploy-bots-and-operation.ts",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format --write src/",
    "format:check": "biome format src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^24.6.2",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@medplum/bot-layer": "^4.4.1",
    "@medplum/core": "^4.4.1",
    "@medplum/definitions": "^4.4.1",
    "@medplum/fhirtypes": "^4.4.1",
    "@medplum/mock": "^4.4.1",
    "dotenv": "^17.2.3",
    "esbuild": "^0.25.10",
    "fast-glob": "^3.3.3",
    "jose": "^6.1.0",
    "kill-the-clipboard": "^1.0.0"
  }
}
